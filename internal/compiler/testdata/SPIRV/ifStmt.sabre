package main

func simpleIfStmt(a bool) int {
	if a {
		return 1
	}
	return 2
}

func ifStmtWithElse(a bool) int {
	if a {
		return 1
	} else {
		return 2
	}
}

func ifStmtWithEmptyElse(a bool) int {
	if a {
		return 1
	} else {
	}
	return 2
}

func ifStmtWithElseIf(a, b bool) int {
	if a {
		return 1
	} else if b {
		return 2
	} else {
		return 3
	}
}
